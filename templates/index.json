    "dynamic_rich_text_zbzUPr": {
      "type": "dynamic-rich-text",
      "custom_css": [],
      "settings": {
        "title": "Lebendige und gesunde Böden, biologischer Anbau, Handlese, spontane Vergärung, und viel Zeit sind dafür unerlässlich",
